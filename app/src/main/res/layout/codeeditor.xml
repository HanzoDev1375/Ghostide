<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:id="@+id/Coordinator">

    <ImageView
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:scaleType="centerCrop"
        android:id="@+id/icon_backgroundghost" />
    <ir.ninjacoder.prograsssheet.VideoSurfaceView
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:id="@+id/videoback"
        android:visibility="gone"
    />
    <LinearLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        android:orientation="vertical"
        android:id="@+id/getColorPass">

        <LinearLayout
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:orientation="vertical">

            <ir.ninjacoder.ghostide.widget.PraramnetLayoutNinja
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:gravity="center_vertical"
                android:orientation="vertical"
                android:id="@+id/Mainlinear">

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:gravity="center_horizontal|center_vertical"
                    android:orientation="vertical"
                    android:id="@+id/multytab">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:gravity="center_horizontal|center_vertical"
                        android:orientation="horizontal"
                        android:translationX="2.0dp"
                        android:id="@+id/CustomToolbar">

                        <LinearLayout
                            android:layout_height="wrap_content"
                            android:layout_width="match_parent"
                            android:gravity="center_vertical"
                            android:padding="4dp"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:gravity="center_vertical"
                                android:padding="8dp"
                                android:orientation="horizontal">

                                <ImageView
                                    android:layout_height="50dp"
                                    android:layout_width="50dp"
                                    android:padding="5dp"
                                    android:orientation="horizontal"
                                    android:id="@+id/avatargithubuser" />

                                <TextView
                                    android:layout_height="wrap_content"
                                    android:layout_width="wrap_content"
                                    android:padding="8dp"
                                    android:ellipsize="end"
                                    android:textSize="16sp"
                                    android:singleLine="true"
                                    android:layout_weight="2"
                                    android:id="@+id/titleauthor" />

                                <ImageView
                                    android:layout_height="25dp"
                                    android:layout_width="25dp"
                                    android:src="@drawable/okeye"
                                    android:scaleType="centerInside"
                                    android:layout_marginLeft="3dp"
                                    android:layout_marginRight="3dp"
                                    android:id="@+id/image" />

                                <ImageView
                                    android:layout_height="25dp"
                                    android:layout_width="25dp"
                                    android:src="@drawable/redo"
                                    android:scaleType="fitCenter"
                                    android:layout_marginLeft="3dp"
                                    android:layout_marginRight="3dp"
                                    android:id="@+id/redo" />

                                <ImageView
                                    android:layout_height="25dp"
                                    android:layout_width="25dp"
                                    android:src="@drawable/undos"
                                    android:scaleType="fitCenter"
                                    android:layout_marginLeft="3dp"
                                    android:layout_marginRight="3dp"
                                    android:id="@+id/undo" />

                                <ImageView
                                    android:layout_height="25dp"
                                    android:layout_width="25dp"
                                    android:src="@drawable/ic_more_vert_white"
                                    android:scaleType="fitCenter"
                                    android:layout_marginRight="4dp"
                                    android:id="@+id/menupopnew" />

                            </LinearLayout>

                        </LinearLayout>

                    </LinearLayout>

                    <ProgressBar
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:indeterminate="true"
                        android:padding="8dp"
                        style="?android:progressBarStyleHorizontal"
                        android:id="@+id/progressbar1" />

                    <com.google.android.material.tabs.TabLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:background="@android:color/transparent"
                        app:tabGravity="fill"
                        style="@style/AppTheme.TabLayout"
                        app:tabTextAppearance="@android:style/TextAppearance.Widget.TabWidget"
                        android:id="@+id/tablayouteditor" />

                    <androidx.recyclerview.widget.RecyclerView
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:padding="8dp"
                        android:id="@+id/dir" />

                </LinearLayout>

                <FrameLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="5"
                    android:id="@+id/FrameLayout01">

                    <ir.ninjacoder.ghostide.IdeEditor
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:id="@+id/editor" />

                    <ImageView
                        android:layout_height="match_parent"
                        android:layout_width="match_parent"
                        android:scaleType="centerInside"
                        android:id="@+id/imageloadereditor" />

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:gravity="right|center_vertical"
                        android:orientation="horizontal"
                        android:id="@+id/FrameLayout02"
                        android:layout_gravity="right">

                        <LinearLayout
                            android:layout_height="wrap_content"
                            android:layout_width="wrap_content"
                            android:gravity="right|center_vertical"
                            android:padding="8dp"
                            android:id="@+id/linear3">

                            <ImageView
                                android:layout_height="30dp"
                                android:layout_width="30dp"
                                android:layout_marginBottom="8dp"
                                android:scaleType="centerInside"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                android:layout_marginTop="8dp"
                                android:layout_gravity="right|center_vertical"
                                android:id="@+id/iconAuthor" />

                            <ProgressBar
                                android:layout_height="30dp"
                                android:layout_width="30dp"
                                android:layout_marginBottom="8dp"
                                android:indeterminate="false"
                                android:layout_marginLeft="5dp"
                                android:layout_marginRight="5dp"
                                style="?android:progressBarStyle"
                                android:layout_marginTop="8dp"
                                android:id="@+id/proanjctor"
                                android:layout_gravity="right|center_vertical" />

                        </LinearLayout>

                    </LinearLayout>

                </FrameLayout>

                <LinearLayout
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:gravity="center_vertical"
                    android:padding="3dp"
                    android:orientation="horizontal"
                    android:id="@+id/newLayoutSymbolBar">

                    <LinearLayout
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_height="0dp"
                            android:layout_width="match_parent"
                            android:background="@android:color/transparent"
                            android:gravity="right"
                            android:padding="2dp"
                            android:orientation="vertical"
                            android:id="@+id/barSymoble">

                            <ImageView
                                android:layout_height="25dp"
                                android:layout_width="25dp"
                                android:src="@drawable/close"
                                android:scaleType="centerInside"
                                android:padding="3dp"
                                android:layout_marginLeft="2dp"
                                android:layout_marginRight="2dp"
                                android:id="@+id/imageview1" />

                            <LinearLayout
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:gravity="center_vertical"
                                android:padding="8dp"
                                android:orientation="horizontal">

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_height="match_parent"
                            android:layout_width="match_parent"
                            android:orientation="vertical">

                            <ir.ninjacoder.ghostide.marco.GhostWebEditorSearch
                                android:layout_height="match_parent"
                                android:layout_width="wrap_content"
                                android:padding="8dp"
                                android:id="@+id/editor_ser" />

                            <androidx.recyclerview.widget.RecyclerView
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent"
                                android:padding="8dp"
                                android:id="@+id/syspiar" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

            </ir.ninjacoder.ghostide.widget.PraramnetLayoutNinja>

        </LinearLayout>

    </LinearLayout>

    <ir.ninjacoder.ghostide.widget.ExrtaFab
        android:layout_height="80dp"
        android:layout_width="80dp"
        android:layout_marginBottom="44dp"
        app:backgroundTint="?colorPrimary"
        android:padding="24dp"
        app:iconTint="?colorSurface"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginTop="18dp"
        android:id="@+id/_fab"
        android:layout_gravity="right|bottom" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>
